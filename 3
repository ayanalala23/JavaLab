import java.util.Scanner;
public class Main{
    public static void main(String args[]){
        
        System.out.println("Enter any two numbers");
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c;
        c=a;
        a=b;
        b=c;
        System.out.println("The swapped numbers are:"+a+" "+"and"+" "+b);
    }
}
